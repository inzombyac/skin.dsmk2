<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<window id="145">
    <defaultcontrol always="true">11</defaultcontrol>
    <zorder>5</zorder>
    <controls>
        <control type="group">
            <include>Animation.FadeIn.Slide</include>
            <centerleft>50%</centerleft>
            <posy>0</posy>
            <width>680</width>
            <height>154</height>
            <control type="image">
                <colordiffuse>$VAR[OSDPanel]</colordiffuse>
                <texture border="16">common/rounded-shadow8.png</texture>
            </control>
            <control type="label" id="10">
                <description>Heading</description>
                <top>20</top>
                <left>30</left>
                <right>30</right>
                <font>MediumBold</font>
                <textcolor>$VAR[OSDPanelWhite70]</textcolor>
            </control>
            <control type="label" id="12">
                <description>Label</description>
                <top>20</top>
                <left>30</left>
                <right>30</right>
                <font>MediumBold</font>
                <textcolor>$VAR[OSDPanelWhite70]</textcolor>
                <align>right</align>
            </control>
            <control type="slider" id="11">
				<orientation>horizontal</orientation>
                <description>Slider</description>
                <posy>100</posy>
                <left>30</left>
                <width>620</width>
                <onleft>11</onleft>
                <onright>11</onright>
                <height>18</height>
                <texturesliderbar colordiffuse="$VAR[ColorHighlight]">osd/progress-end.png</texturesliderbar>
                <textureslidernib>osd/progress-mid.png</textureslidernib>
                <textureslidernibfocus>osd/progress-mid.png</textureslidernibfocus>
            </control>
        </control>
    </controls>
</window>
