<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<includes>
    <constant name="NavBarPad">112</constant>
    <constant name="SidePad">80</constant>
    <constant name="TopPad">60</constant>
    <constant name="PosterPad">216</constant>
    <constant name="PosterH">640</constant>
    <constant name="PosterW">428</constant>
    <constant name="PosterShadowH">688</constant>
    <constant name="PosterShadowW">475</constant>
    <constant name="PosterPanel">750</constant>
    <constant name="ListX">800</constant>
    <constant name="ListW">1000</constant>
    <constant name="ListSquareX">752</constant>
    <constant name="ListSquareW">1048</constant>
    <constant name="ListPosterPad">220</constant>
    <constant name="ListPosterH">500</constant>
    <constant name="ListPosterW">345</constant>
    <constant name="ListPosterShadowH">384</constant>
    <constant name="ListPosterShadowW">387</constant>
    <default type="label">
        <height>66</height>
        <aligny>center</aligny>
        <font>Small</font>
        <textcolor>Black70</textcolor>
        <focusedcolor>Black70</focusedcolor>
        <selectedcolor>$VAR[ColorHighlight2]</selectedcolor>
        <disabledcolor>Black30</disabledcolor>
    </default>
    <default type="edit">
        <height>66</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>Small</font>
        <textoffsetx>30</textoffsetx>
        <textcolor>White70</textcolor>
        <focusedcolor>$VAR[ColourSelected]</focusedcolor>
        <selectedcolor>$VAR[ColorHighlight2]</selectedcolor>
        <disabledcolor>White12</disabledcolor>
        <texturefocus colordiffuse="White30">common/white.png</texturefocus>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="fadelabel">
        <height>66</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>Small</font>
        <textcolor>White70</textcolor>
        <focusedcolor>$VAR[ColourSelected]</focusedcolor>
        <selectedcolor>$VAR[ColorHighlight2]</selectedcolor>
        <disabledcolor>White12</disabledcolor>
        <scrollout>false</scrollout>
        <pauseatend>2000</pauseatend>
    </default>
    <default type="image"></default>
    <default type="button">
        <height>66</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>Small</font>
        <textoffsetx>30</textoffsetx>
        <textcolor>Black70</textcolor>
        <focusedcolor>$VAR[ColourSelected]</focusedcolor>
        <disabledcolor>Black30</disabledcolor>
        <selectedcolor>$VAR[ColorHighlight2]</selectedcolor>
        <texturefocus colordiffuse="White12">common/white.png</texturefocus>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="togglebutton">
        <height>66</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>Small</font>
        <textoffsetx>30</textoffsetx>
        <textcolor>Black70</textcolor>
        <focusedcolor>$VAR[ColourSelected]</focusedcolor>
        <selectedcolor>$VAR[ColorHighlight2]</selectedcolor>
        <disabledcolor>Black30</disabledcolor>
        <texturefocus colordiffuse="White12">common/white.png</texturefocus>
        <alttexturefocus colordiffuse="White12">common/white.png</alttexturefocus>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="radiobutton">
        <height>66</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>Small</font>
        <radioposx>1250</radioposx>
        <radiowidth>32</radiowidth>
        <radioheight>32</radioheight>
        <textoffsetx>30</textoffsetx>
        <textcolor>Black70</textcolor>
        <focusedcolor>$VAR[ColourSelected]</focusedcolor>
        <disabledcolor>Black30</disabledcolor>
        <selectedcolor>$VAR[ColorHighlight2]</selectedcolor>
        <texturefocus colordiffuse="White12">common/white.png</texturefocus>
        <textureradiofocus colordiffuse="Black70">buttons/radio-on.png</textureradiofocus>
        <textureradionofocus colordiffuse="Black30">buttons/radio-off.png</textureradionofocus>
        <textureradioondisabled colordiffuse="Black12">buttons/radio-on.png</textureradioondisabled>
        <textureradioofdisabled colordiffuse="Black12">buttons/radio-off.png</textureradioofdisabled>
        <textoffsetx>30</textoffsetx>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="spincontrolex">
        <height>66</height>
        <font>Small</font>
        <spinwidth>32</spinwidth>
        <spinheight>32</spinheight>
        <textcolor>Black70</textcolor>
        <focusedcolor>$VAR[ColourSelected]</focusedcolor>
        <disabledcolor>Black12</disabledcolor>
        <selectedcolor>$VAR[ColorHighlight2]</selectedcolor>
        <texturefocus colordiffuse="White12">common/white.png</texturefocus>
        <textureup flipy="true" colordiffuse="Black30">buttons/spin-down.png</textureup>
        <texturedown colordiffuse="Black30">buttons/spin-down.png</texturedown>
        <textureupfocus flipy="true" colordiffuse="$VAR[ColourSelected]">buttons/spin-down.png</textureupfocus>
        <texturedownfocus colordiffuse="$VAR[ColourSelected]">buttons/spin-down.png</texturedownfocus>
        <textureupdisabled flipy="true" colordiffuse="Black12">buttons/spin-down.png</textureupdisabled>
        <texturedowndisabled colordiffuse="Black12">buttons/spin-down.png</texturedowndisabled>
        <textoffsetx>30</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="spincontrol">
        <height>66</height>
        <font>Small</font>
        <spinwidth>32</spinwidth>
        <spinheight>32</spinheight>
        <textcolor>Black70</textcolor>
        <focusedcolor>$VAR[ColourSelected]</focusedcolor>
        <disabledcolor>Black12</disabledcolor>
        <selectedcolor>$VAR[ColorHighlight2]</selectedcolor>
        <texturefocus colordiffuse="White12">common/white.png</texturefocus>
        <textureup flipy="true" colordiffuse="Black30">buttons/spin-down.png</textureup>
        <texturedown colordiffuse="Black30">buttons/spin-down.png</texturedown>
        <textureupfocus flipy="true" colordiffuse="$VAR[ColourSelected]">buttons/spin-down.png</textureupfocus>
        <texturedownfocus colordiffuse="$VAR[ColourSelected]">buttons/spin-down.png</texturedownfocus>
        <textureupdisabled flipy="true" colordiffuse="Black12">buttons/spin-down.png</textureupdisabled>
        <texturedowndisabled colordiffuse="Black12">buttons/spin-down.png</texturedowndisabled>
        <textoffsetx>30</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="progress">
        <texturebg colordiffuse="2f5f5f5f">osd/progress-bg.png</texturebg>
        <lefttexture colordiffuse="$VAR[ColorHighlight]">osd/progress-end.png</lefttexture>
        <midtexture colordiffuse="$VAR[ColorHighlight]">osd/progress-end.png</midtexture>
        <righttexture colordiffuse="PanelWhite70">osd/progress-end.png</righttexture>
    </default>
    <default type="slider">
        <width>100%</width>
        <height>20</height>
        <texturesliderbar colordiffuse="00ffffff">osd/progress-end.png</texturesliderbar>
        <textureslidernib>osd/progress-mid.png</textureslidernib>
        <textureslidernibfocus>osd/progress-mid.png</textureslidernibfocus>
        <orientation>horizontal</orientation>
    </default>
    <default type="sliderex">
        <height>66</height>
        <selectedcolor>$VAR[ColorHighlight2]</selectedcolor>
        <sliderwidth>230</sliderwidth>
        <sliderheight>23</sliderheight>
        <texturesliderbar colordiffuse="Dark1">osd/progress-bg.png</texturesliderbar>
        <textureslidernib colordiffuse="Light4">osd/progress-end.png</textureslidernib>
        <textureslidernibfocus colordiffuse="Light1">osd/progress-end.png</textureslidernibfocus>
        <font>Small</font>
        <aligny>center</aligny>
        <textcolor>Black70</textcolor>
        <focusedcolor>$VAR[ColourSelected]</focusedcolor>
        <disabledcolor>Black12</disabledcolor>
        <textoffsetx>30</textoffsetx>
        <textoffsety>0</textoffsety>
    </default>
    <default type="scrollbar">
        <width>8</width>
        <texturesliderbackground colordiffuse="White12">common/white.png</texturesliderbackground>
        <texturesliderbar colordiffuse="White30">common/white.png</texturesliderbar>
        <texturesliderbarfocus colordiffuse="$VAR[ColorHighlight]">common/white.png</texturesliderbarfocus>
        <showonepage>false</showonepage>
        <orientation>vertical</orientation>
    </default>
    <default type="textbox">
        <font>Small</font>
        <textcolor>Black70</textcolor>
        <selectedcolor>$VAR[ColorHighlight2]</selectedcolor>
        <pagecontrol>61</pagecontrol>
        <autoscroll delay="5000" time="1000" repeat="5000"/>
    </default>
</includes>
