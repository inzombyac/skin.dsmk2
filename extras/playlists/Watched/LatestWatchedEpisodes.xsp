<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
    <name>Latest Watched Episodes</name>
    <match>all</match>
    <rule field="playcount" operator="greaterthan">
        <value>0</value>
    </rule>
    <limit>5</limit>
    <order direction="descending">lastplayed</order>
</smartplaylist>
